.box{
    display flex
    justify-content center
    margin-top 10px
    &>div{
        width 1190px
        height 540px
        background-color #fff
        display flex
        &>.left{
            width 590px
            height 540px
            &>.title{
                width 100%
                height 24px
                margin 10px
                &>span{
                    font-size 12px
                    color #999999
                    font-weight 400
                    margin-left 5px
                }
            }
            &>.content{
                width 590px
                height 496px
                display flex
                flex-wrap: wrap 
                

                &>div{
                    width 271px
                    height 248px
                    padding: 6px 0 20px 0;
                    margin-left: 8px;
                    
                    // background-color green
                    &>span{
                        font-size: 16px;
                        vertical-align: top;
                        color: #333;
                        line-height: 42px;
                    }
                    &>div{
                        display flex
                        width 100%
                        height 100%
                        
                        
                        &>img{
                            width 180px
                            height 180px
                        }
                        &>:last-child{
                            width 89px
                            height 180px
                            &>img{
                                width 89px
                                height 89px
                                margin 0px 2px 0px 2px
                            }
                        }
                    }
                    
                }
            }
        }
        &>.right{
            width 590px
            height 540px
            &>.title{
                width 100%
                height 24px
                margin 10px
                &>span{
                    font-size 12px
                    color #999999
                    font-weight 400
                    margin-left 5px
                }
            }
            &>.content{
                width 590px
                height 496px
                display flex
                flex-wrap: wrap 

                &>div{
                    width 271px
                    height 248px
                    padding: 6px 0 20px 0;
                    margin-left: 8px;
                    // background-color green
                    &>span{
                        font-size: 16px;
                        vertical-align: top;
                        color: #333;
                        line-height: 42px;
                        height 42px
                    }
                    &>div{
                        display flex
                        width 100%
                        height 100%
                        // background-color red
                        &>img{
                            width 180px
                            height 180px
                        }
                        &>div{
                            width 89px
                            height 180px
                            &>img{
                                width 89px
                                height 89px
                                margin 0px 2px 0px 2px
                            }
                        }
                    }
                }
            }
        }
    }
}